{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/duggu-api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
